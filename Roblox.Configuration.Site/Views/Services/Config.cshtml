@model IEnumerable<Roblox.ApiControlPlane.Client.IService>
@section scripts {
    @Scripts.Render("~/Scripts/Services.js")
    @Styles.Render("~/bundles/config-styles")
}
@{
    ViewBag.Title = "Services Configuration";
}


<div>
    <h3>Services</h3>
    <span id="service-default-number">@Model.Count() services found.</span>
    <div id="servicesResultsDiv" class="resultsContainer">
        @Html.Partial("Table", Model)
    </div>
</div>
